{
    "name": "ytb-dw/php-sdk",
    "description": "SDK PHP officiel pour l'API YouTube Downloader",
    "type": "library",
    "keywords": ["youtube", "downloader", "video", "audio", "mp3", "mp4", "api", "sdk"],
    "homepage": "https://github.com/ytb-dw/php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "YTB-DW Team",
            "email": "support@ytb-dw.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "psalm/phar": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "YtbDw\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YtbDw\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "psalm": "psalm",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
