{
    "name": "ytb-dw/sdk-php",
    "description": "SDK PHP pour l'API de Mon Service",
    "type": "library",
    "keywords": ["sdk", "api", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Votre Nom",
            "email": "Devd00237@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "MonSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MonSDK\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
